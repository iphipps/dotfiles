{
  "coc.preferences.formatOnSaveFiletypes": ["css", "Markdown", "rust"],
  "coc.preferences.useQuickfixForLocations": false,
  "coc.preferences.jumpCommand": "tab drop",
  "coc.source.file.ignoreHidden": false,
  "highlight.colors.enable": true,
  "highlight.colorNames.enable": true,
  "colors.filetypes": ["*"],
  "coc.preferences.colorSupport": true,
  "diagnostic.errorSign": "üö´",
  "diagnostic.warningSign": "‚ö†Ô∏è",
  "diagnostic.infoSign": "üíÅ",
  "eslint.autoFixOnSave": false,
  "rust.clippy_preference": "on",
  "git.realtimeGutters": true,

  "codeLens.enable": true,
  "list.insertMappings": {
    "<C-x>": "action:split",
    "<C-v>": "action:vsplit"
  },
  "list.normalMappings": {
    "s": "action:split",
    "v": "action:vsplit"
  },

  "list.source.files.command": "fd",
  "list.source.files.args": ["--hidden", "--type", "f"],
  "list.maxPreviewHeight": 50,
  "list.previewSplitRight": true,
  "suggest.enablePreview": true,
  "tsserver.log": "verbose",
  "tsserver.enableJavascript": true,
  "tsserver.formatOnType": false,
  "typescript.format.enabled": false,
  "diagnostic.refreshAfterSave": false
}
