{
  "coc.preferences.formatOnSaveFiletypes": ["css", "Markdown", "rust"],
  "coc.preferences.formatOnInsertLeave": false,
  "coc.preferences.useQuickfixForLocations": false,
  "diagnostic.errorSign": "🚫",
  "diagnostic.warningSign": "⚠️",
  "diagnostic.infoSign": "💁",
  "eslint.autoFix": false,
  "eslint.autoFixOnSave": false,
  "rust.clippy_preference": "on",
  "git.realtimeGutters": true,

  "codeLens.enable": true,
  "list.insertMappings": {
    "<C-x>": "action:split",
    "<C-v>": "action:vsplit"
  },
  "list.normalMappings": {
    "s": "action:split",
    "v": "action:vsplit"
  },
  "list.source.files.command": "fdfind",
  "list.source.files.args": ["--hidden", "--type", "f"],
  "list.maxPreviewHeight": 32,
  "list.previewSplitRight": true,
  "suggest.enablePreview": true,
  "tsserver.enableJavascript": true,
  "tsserver.formatOnType": false,
  "typescript.format.enabled": false,
  "diagnostic.refreshAfterSave": false
}
